{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd V2-Web && npm ci && npm run build",
  "devCommand": "cd V2-Web && npm run dev",
  "installCommand": "cd V2-Web && npm ci",
  "framework": "nextjs",
  "outputDirectory": "V2-Web/.next",
  "rootDirectory": "V2-Web",
  "functions": {
    "V2-Web/src/app/api/scrape/route.ts": {
      "maxDuration": 300
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
